[
  {
    "src": {
      "globs": [
        "tests/source/**/*.ejs"
      ],
      "options": {
        "allowEmpty": true,
        "sourcemaps": true
      }
    },
    "ejs": {},
    "wrap": {
      "template": "`<%= contents %>`",
      "data": {},
      "options": {
        "namespace": "Tests",
        "imports": {
          "processPartialName": {
            "filePath": {
              "location": "relative",
              "replace": [
                {
                  "from": "tests/source/",
                  "to": ""
                },
                {
                  "from": ".ejs",
                  "to": ""
                }
              ],
              "transform": [
                {
                  "from": "dashCase",
                  "to": "camelCase",
                  "options": {
                    "first": "upperCase"
                  }
                }
              ]
            }
          }
        }
      }
    },
    "declare": {
      "options": {
        "noRedeclare": true,
        "namespace": "Tests",
        "processName": {
          "filePath": {
            "location": "relative",
            "replace": [
              {
                "from": "tests/source/",
                "to": ""
              },
              {
                "from": ".ejs",
                "to": ""
              }
            ],
            "transform": [
              {
                "from": "dashCase",
                "to": "camelCase",
                "options": {
                  "first": "upperCase"
                }
              }
            ]
          }
        }
      }
    },
    "concat": {
      "options": {
        "path": "test-1-templates.js"
      }
    },
    "dest": [
      ["tests/distribute/test-1/scripts"],
      {
        "sourcemaps": true
      }
    ]
  }
]
