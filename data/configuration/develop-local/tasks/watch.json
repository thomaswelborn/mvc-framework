{
  "reload": {
    "globs": [
      "utils/scripts/**/*.js",
      "gulpfile.js/**/*.js",
      "data/**/*.{js,json}"
    ],
    "options": {},
    "task": {
      "type": "series",
      "tasks": [
        "Spawn"
      ]
    }
  },
  "scripts": {
    "globs": [
      "source/scripts/**/*.js"
    ],
    "options": {},
    "task": {
      "type": "series",
      "tasks": [
        "Scripts"
      ]
    }
  },
  "scripts-distribute": {
    "globs": [
      "distribute/**/*.{map,js}"
    ],
    "options": {},
    "task": {
      "type": "series",
      "tasks": [
        "Copy"
      ]
    }
  }
}
