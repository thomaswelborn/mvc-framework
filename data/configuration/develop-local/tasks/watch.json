{
  "reload": {
    "globs": [
      "utils/scripts/**/*.js",
      "gulpfile.js/**/*.js",
      "data/**/*.{js,json}"
    ],
    "options": {},
    "task": {
      "type": "series",
      "tasks": [
        "spawn"
      ]
    }
  },
  "scripts": {
    "globs": [
      "source/scripts/**/*.js"
    ],
    "options": {},
    "task": {
      "type": "series",
      "tasks": [
        "scripts"
      ]
    }
  },
  "scripts-distribute": {
    "globs": [
      "distribute/**/*.{map,js}"
    ],
    "options": {},
    "task": {
      "type": "series",
      "tasks": [
        "copy"
      ]
    }
  }
}
