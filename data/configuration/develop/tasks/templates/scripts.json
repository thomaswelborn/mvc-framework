[
  {
    "src": {
      "globs": [
        "source/scripts/templates/**/*.hbs"
      ],
      "options": {}
    },
    "handlebars": {},
    "declare": {
      "layouts": {
        "options": {
          "namespace": "MVC.Templates",
          "noRedeclare": true,
          "processName": {
            "filePath": {
              "location": "relative",
              "replace": [
                {
                  "from": "source/scripts/templates",
                  "to": ""
                },
                {
                  "from": ".js",
                  "to": ""
                }
              ],
              "transform": [
                {
                  "from": "dashCase",
                  "to": "camelCase",
                  "options": {
                    "first": "upperCase"
                  }
                }
              ]
            }
          }
        }
      },
      "partials": {}
    },
    "concat": {
      "options": {
        "path": "templates.js"
      }
    },
    "wrap": {
      "layouts": {
        "template": "Handlebars.template(<%= contents %>)",
        "data": {},
        "options": {
          "imports": {
            "processPartialName": {
              "filePath": {
                "location": "relative",
                "replace": [
                  {
                    "from": "source/scripts/templates",
                    "to": ""
                  },
                  {
                    "from": ".js",
                    "to": ""
                  }
                ],
                "transform": [
                  {
                    "from": "dashCase",
                    "to": "camelCase",
                    "options": {
                      "first": "upperCase"
                    }
                  }
                ]
              }
            }
          }
        }
      },
      "partials": {
        "template": "Handlebars.registerPartial(<%= processPartialName(file.relative) %>, Handlebars.template(<%= contents %>))",
        "data": {},
        "options": {
          "imports": {
            "processPartialName": {
              "filePath": {
                "location": "relative",
                "replace": [
                  {
                    "from": "source/scripts/templates",
                    "to": ""
                  },
                  {
                    "from": ".js",
                    "to": ""
                  }
                ],
                "transform": [
                  {
                    "from": "dashCase",
                    "to": "camelCase",
                    "options": {
                      "first": "upperCase"
                    }
                  }
                ]
              }
            }
          }
        }
      }
    },
    "dest": "develop/scripts"
  }
]
