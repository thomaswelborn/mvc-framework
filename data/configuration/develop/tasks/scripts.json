{
  "mvc": {
    "src": {
      "globs": [
        "source/scripts/modules/MVC/MVC.js",
        "source/scripts/modules/MVC/Constants/Constants.js",
        "source/scripts/modules/MVC/Constants/Events.js",
        "source/scripts/modules/MVC/Templates/Templates.js",
        "source/scripts/modules/MVC/Utils/Utils.js",
        "source/scripts/modules/MVC/Utils/is.js",
        "source/scripts/modules/MVC/Utils/typeOf.js",
        "source/scripts/modules/MVC/Utils/addPropertiesToObject.js",
        "source/scripts/modules/MVC/Utils/objectQuery.js",
        "source/scripts/modules/MVC/Utils/toggleEventsForTargetObjects.js",
        "source/scripts/modules/MVC/Utils/validateDataSchema.js",
        "source/scripts/modules/MVC/Events.js",
        "source/scripts/modules/MVC/Channels/Channels.js",
        "source/scripts/modules/MVC/Channels/Channel/Channel.js",
        "source/scripts/modules/MVC/Base.js",
        "source/scripts/modules/MVC/Observer.js",
        "source/scripts/modules/MVC/Service.js",
        "source/scripts/modules/MVC/Model.js",
        "source/scripts/modules/MVC/Emitter.js",
        "source/scripts/modules/MVC/View.js",
        "source/scripts/modules/MVC/Controller.js",
        "source/scripts/modules/MVC/Router.js"
      ],
      "options": {}
    },
    "delSync": {
      "src": {
        "globs": [
          "develop/scripts/**/*.js"
        ]
      }
    },
    "babel": {
      "options": {
        "presets": ["@babel/env"]
      }
    },
    "sourcemaps": {
      "init": {
        "options": {}
      },
      "write": {
        "options": {
          "sourceMappingURLPrefix": "http://localhost:3000",
          "includeContent": true
        },
        "dest": "../.maps/scripts"
      }
    },
    "concat": {
      "options": {
        "path": "mvc-framework.js"
      }
    },
    "dest": "develop/scripts",
    "browserSync": {
      "stream": true
    }
  }
}
