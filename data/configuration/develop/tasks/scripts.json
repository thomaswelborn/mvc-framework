{
  "mvc": {
    "src": {
      "globs": [
        "source/scripts/MVC.js",
        "source/scripts/Events.js",
        "source/scripts/Service.js",
        "source/scripts/Channels/Channels.js",
        "source/scripts/Channels/Channel/Channel.js",
        "source/scripts/Model.js",
        "source/scripts/View.js",
        "source/scripts/Controller.js",
        "source/scripts/Router.js"
      ],
      "options": {}
    },
    "delSync": {
      "src": {
        "globs": [
          "develop/scripts/**/*.js"
        ]
      }
    },
    "babel": {
      "options": {
        "presets": ["@babel/env"]
      }
    },
    "sourcemaps": {
      "init": {
        "options": {}
      },
      "write": {
        "options": {
          "sourceMappingURLPrefix": "http://localhost:3000",
          "includeContent": true
        },
        "dest": "../.maps/scripts"
      }
    },
    "concat": {
      "options": {
        "path": "mvc-framework.js"
      }
    },
    "dest": "develop/scripts",
    "browserSync": {
      "stream": true
    }
  }
}
