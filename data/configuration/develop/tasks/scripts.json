{
  "mvc": {
    "src": {
      "globs": [
        "source/scripts/MVC.js",
        "source/scripts/Events.js"
      ],
      "options": {}
    },
    "delSync": {
      "src": {
        "globs": [
          "develop/scripts/**/*.js"
        ]
      }
    },
    "concat": {
      "options": {
        "path": "mvc-framework.js"
      }
    },
    "dest": "develop/scripts",
    "browserSync": {
      "stream": true
    }
  }
}
