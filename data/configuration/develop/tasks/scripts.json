{
  "mvc": {
    "src": {
      "globs": [
        "source/scripts/MVC/MVC.js",
        "source/scripts/MVC/Utils.js",
        "source/scripts/MVC/Events.js",
        "source/scripts/MVC/Service.js",
        "source/scripts/MVC/Channels/Channels.js",
        "source/scripts/MVC/Channels/Channel/Channel.js",
        "source/scripts/MVC/Observers/Observers.js",
        "source/scripts/MVC/Observers/Observer/Observer.js",
        "source/scripts/MVC/Model.js",
        "source/scripts/MVC/View.js",
        "source/scripts/MVC/Controller.js",
        "source/scripts/MVC/Router.js"
      ],
      "options": {}
    },
    "delSync": {
      "src": {
        "globs": [
          "develop/scripts/**/*.js"
        ]
      }
    },
    "babel": {
      "options": {
        "presets": ["@babel/env"]
      }
    },
    "sourcemaps": {
      "init": {
        "options": {}
      },
      "write": {
        "options": {
          "sourceMappingURLPrefix": "http://localhost:3000",
          "includeContent": true
        },
        "dest": "../.maps/scripts"
      }
    },
    "concat": {
      "options": {
        "path": "mvc-framework.js"
      }
    },
    "dest": "develop/scripts",
    "browserSync": {
      "stream": true
    }
  }
}
