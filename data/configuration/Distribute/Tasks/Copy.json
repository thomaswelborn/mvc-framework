{
  "mvc-framework-distribute": {
    "src": {
      "globs": [
        "distribute/**/*"
      ],
      "options": {
        "allowEmpty": false
      }
    },
    "dest": [
      [
        "../mvc-framework-demonstration/node_modules/mvc-framework/distribute"
      ],
      {}
    ]
  },
  "mvc-framework-distribute-source": {
    "src": {
      "globs": [
        "source/**/*"
      ],
      "options": {
        "allowEmpty": false
      }
    },
    "dest": [
      [
        "../mvc-framework-demonstration/node_modules/mvc-framework/source"
      ],
      {}
    ]
  }
}
