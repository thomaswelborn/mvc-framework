{
  "browser": {
    "src": {
      "globs": [
        "source/MVC/index.js"
      ],
      "options": {
        "allowEmpty": true,
        "sourcemaps": true
      }
    },
    "rollup": {
      "options": {
        "input": "source/MVC/index.js",
        "plugins": [
          [
            "rollupPluginBabel",
            {
              "babelrc": false,
              "plugins": [
                [
                  "@babel/plugin-proposal-class-properties",
                  {
                    "loose": false
                  }
                ],
                [
                  "@babel/plugin-proposal-export-default-from"
                ]
              ],
              "presets": [
                [
                  "@babel/preset-env",
                  {
                    "targets": {
                      "esmodules": true
                    }
                  }
                ]
              ]
            }
          ],
          [
            "rollupPluginEJS",
            {
              "compilerOptions": {
                "client": true
              }
            }
          ]
        ],
        "output": {
          "file": "distribute/browser/mvc-framework.js",
          "name": "MVC",
          "format": "umd",
          "sourcemap": "inline"
        }
      }
    },
    "concat": {
      "options": {
        "path": "browser/mvc-framework.js"
      }
    },
    "minify": {
      "options": {}
    },
    "dest": [
      ["distribute"],
      {
        "sourcemaps": true
      }
    ]
  }
}
